  </path>
  
  
  <patternset id="excluded.from.module.gomoku2">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.gomoku2">
    <patternset refid="excluded.from.module.gomoku2"/>
  </patternset>
  
  <path id="gomoku2.module.sourcepath">
    <dirset dir="${module.gomoku2.basedir}">
      <include name="src/main/scala"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="gomoku2.module.test.sourcepath">
    <dirset dir="${module.gomoku2.basedir}">
      <include name="src/test/scala"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.gomoku2" depends="compile.module.gomoku2.production,compile.module.gomoku2.tests" description="Compile module gomoku2"/>
  
  <target name="compile.module.gomoku2.production" depends="register.custom.compilers" description="Compile module gomoku2; production classes">
    <mkdir dir="${gomoku2.output.dir}"/>
    <javac2 destdir="${gomoku2.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.gomoku2}/javac">
      <compilerarg line="${compiler.args.gomoku2}"/>
      <bootclasspath refid="gomoku2.module.bootclasspath"/>
      <classpath refid="gomoku2.module.production.classpath"/>
      <src refid="gomoku2.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.gomoku2"/>
    </javac2>
    
    <copy todir="${gomoku2.output.dir}">
      <fileset dir="${module.gomoku2.basedir}/src/main/scala">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.gomoku2.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.gomoku2.tests" depends="register.custom.compilers,compile.module.gomoku2.production" description="compile module gomoku2; test classes" unless="skip.tests">
    <mkdir dir="${gomoku2.testoutput.dir}"/>
    <javac2 destdir="${gomoku2.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.gomoku2}/javac">
      <compilerarg line="${compiler.args.gomoku2}"/>
      <bootclasspath refid="gomoku2.module.bootclasspath"/>
      <classpath refid="gomoku2.module.classpath"/>
      <src refid="gomoku2.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.gomoku2"/>
    </javac2>
    
    <copy todir="${gomoku2.testoutput.dir}">
      <fileset dir="${module.gomoku2.basedir}/src/test/scala">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.gomoku2" description="cleanup module">
    <delete dir="${gomoku2.output.dir}"/>
    <delete dir="${gomoku2.testoutput.dir}"/>
  </target>
  
  
