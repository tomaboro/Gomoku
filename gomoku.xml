  <property name="module.jdk.bin.gomoku2" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.gomoku2" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.gomoku2" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="gomoku2.output.dir" value="${module.gomoku2.basedir}/target/scala-2.12/classes"/>
  <property name="gomoku2.testoutput.dir" value="${module.gomoku2.basedir}/target/scala-2.12/test-classes"/>
  
  <path id="gomoku2.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="gomoku2.module.production.classpath">
    <path refid="${module.jdk.classpath.gomoku2}"/>
