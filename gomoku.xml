  <!-- Project Libraries -->
  
  <path id="library.sbt:_org.scala-lang.modules:scala-swing_2.12:2.0.0-m2:jar.classpath">
    <pathelement location="${basedir}/../../.ivy2/cache/org.scala-lang.modules/scala-swing_2.12/bundles/scala-swing_2.12-2.0.0-M2.jar"/>
  </path>
  
  <path id="library.sbt:_org.scala-lang:scala-library:2.12.3:jar.classpath">
    <pathelement location="${basedir}/../../.ivy2/cache/org.scala-lang/scala-library/jars/scala-library-2.12.3.jar"/>
  </path>
  
  
  <!-- Global Libraries -->
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm-all.jar"/>
    <pathelement location="${javac2.home}/jgoodies-forms.jar"/>
